{
  "name": "discord-bot",
  "version": "0.1.0",
  "main": "main.js",
  "repository": "https://github.com/Ludo-code/discord-bots.git",
  "author": "Ludo-code <lasagne.berthet@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "discordjs/discord.js"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-aqua": "^7.0.1"
  },
  "eslintConfig": {
    "extends": "aqua/node",
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "curly": "off"
    }
  }
}
